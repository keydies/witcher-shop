{
	"extends": ["react-app", "react-app/jest"],
	"rules": {
		"no-console": "warn",
		"prefer-const": "warn",
		"quotes": ["warn", "single"],
		"jsx-quotes": ["warn", "prefer-single"],
		"max-len": ["warn", { "code": 120 }],
		"comma-dangle": [
			"warn",
			{
				"arrays": "never",
				"objects": "never",
				"imports": "never",
				"exports": "never",
				"functions": "never"
			}
		],
		"semi": ["warn", "always"]
	}
}
